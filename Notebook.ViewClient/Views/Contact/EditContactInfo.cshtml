@model Notebook.DTO.Models.Request.ContactInformationModel

@{
    ViewData["Title"] = "Contact Information";
}

<div class="jumbotron">
    <div class="row">
        <form asp-action="CreateContact" class="bold-labels">
            <div class="row">
                <div class="col-12">
                    <partial name="_CreateContactInformation" />
                </div>
                <div class="form-group text-md-right">
                    <a asp-action="Index" asp-controller="Home" class="btn btn-secondary btnRadius">Cancel</a>
                    <input type="submit" value="Submit" class="btn btn-danger btnRadius" />
                </div>
            </div>
        </form>
    </div>
</div>
